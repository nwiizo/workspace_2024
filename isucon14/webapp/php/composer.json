{
  "name": "isucon/isucon14-php",
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-pdo": "*",
    "ext-ctype": "*",
    "monolog/monolog": "^3.7",
    "slim/psr7": "^1.5",
    "slim/slim": "^4.10",
    "symfony/uid": "^7.1",
    "ybelenko/openapi-data-mocker": "^1.0",
    "ybelenko/openapi-data-mocker-server-middleware": "^1.2"
  },
  "require-dev": {
    "jangregor/phpstan-prophecy": "^1.0.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5.26",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "IsuRide\\": "src/"
    },
    "files": [
      "src/function.php"
    ]
  },
  "scripts": {
    "generate": [
      "openapi-generator generate -i ../openapi.yaml -g php -o ./ --config config.yaml"
    ]
  }
}
